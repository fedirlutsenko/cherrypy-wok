                "password"
            ],
            "properties": {
                "username": {
                    "description": "Username",
                    "type": "string",
                    "minLength": 1,
                    "error": "WOKAUTH0003E"
                },
                "password": {
                    "description": "Password",
                    "type": "string",
                    "minLength": 1,
                    "error": "WOKAUTH0006E"
                }
            },
            "additionalProperties": false,
            "error": "WOKAUTH0007E"
        }
    }
}
